{
  "name": "combine-utils",
  "version": "0.1.0",
  "description": "Utility functions to help with combining items.",
  "main": "dist/lib/index.js",
  "repository": "https://github.com/thomasheyenbrock/combine-utils.git",
  "keywords": [
    "combine",
    "utils",
    "combinatorics",
    "collections",
    "items",
    "combinations",
    "sets"
  ],
  "author": "Thomas Heyenbrock <thomas.heyenbrock@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "npm run build && node dist/scripts/benchmark.js",
    "build": "rimraf dist && tsc",
    "lint": "tslint -p .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prettify": "prettier --write '**/*.{js,ts}'",
    "release": "semantic-release",
    "test": "jest --coverage",
    "test:coverage": "npm run test && opn coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "ts": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.3",
    "commitlint": "12.1.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "opn-cli": "5.0.0",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.3",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4"
  }
}
